		"build": "astro build",
		"preview": "astro preview"
	},
	"dependencies": {
		"@astrojs/markdown-component": "1.0.5",
		"@cronitorio/cronitor-rum-js": "0.3.0",
		"astro": "2.10.15"
	}
}
