{
	"name": "@barbajoe/landing",
	"version": "1.0.0",
	"scripts": {
		"all": "pnpm i && pnpm clean && pnpm style-check && pnpm build",
		"clean": "rm -rf dist",
		"lint": "biome ci ./src",
		"format": "biome format --write ./src",
		"style-check": "biome check --write ./src",
